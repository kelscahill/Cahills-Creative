{% if latest_posts %}
  {% for post in latest_posts %}
    {% include "01-molecules/blocks/block--latest.twig"
      with {
        id: post.id,
        thumb_id: get_post_thumbnail_id(post.id),
        image: wp_get_attachment_image_src(post.id, 'thumbnail')[0],
        alt: get_post_meta(post.id, '_wp_attachment_image_alt', true),
        title: post.title,
        link: post.link,
        date: get_the_date('F j, Y', post)
      }
    %}
  {% endfor %}
{% endif %}
